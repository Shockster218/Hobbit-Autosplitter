<Window x:Name="windowMain" x:Class="HobbitAutoSplitter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HobbitAutoSplitter"
        mc:Ignorable="d"
        Title="MainWindow" Height="576" Width="1024" Closed="CloseWindow" MinWidth="1024" MinHeight="576">
    <Grid x:Name="gridMain" Margin="0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="321*"/>
            <ColumnDefinition Width="695*"/>
        </Grid.ColumnDefinitions>
        <Grid x:Name="gridLeft" Margin="10" d:IsHidden="True"/>
        <Grid x:Name="gridRight" Grid.Column="1" Margin="10" d:IsHidden="True">
            <Image x:Name="imageCapture" Margin="0" d:IsHidden="True"/>
        </Grid>
    </Grid>
</Window>
